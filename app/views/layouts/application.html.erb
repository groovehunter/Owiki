<!DOCTYPE html>
<html>
<head>
  <title>Owiki</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>

<div id="header">
  <%= link_to 'Owiki', '/' %>
</div>
  

<div id="menu">
                <div class="menutree">
        <%= render :file =>  'layouts/nav' %>
                </div>
</div>

<div id="content">
<div id="xsnazzy">
<b class="xtop"><b class="xb1"></b><b class="xb2"></b><b class="xb3"></b><b class="xb4"></b></b>
<div class="xboxcontent">
<% if notice %>
  <p id="notice"><%= notice %></p>
<% end %>

<%= yield %>
<br />

</div>
<b class="xbottom"><b class="xb4"></b><b class="xb3"></b><b class="xb2"></b><b class="xb1"></b></b>
</div>
<br class="clear" /><br/>

</div>

<div id="footer">

</div>

</body>
</html>
